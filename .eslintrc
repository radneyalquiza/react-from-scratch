{
    "env": {
        "es6": true,
        "browser": true,
        "node": true
    },
    "extends": "airbnb",
    "parser": "babel-eslint",
    "rules": {
        "react/jsx-filename-extension": 0,
        "react/forbid-prop-types": 0,
        "no-console": 0,
        "no-use-before-define": [
          "error",
          {
            "functions": false
          }
        ]
    }
}